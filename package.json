    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.1.3",
    "opn-cli": "^4.1.0",
    "prettier": "^1.18.2",
    "reify": "^0.18.1",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-multi-entry": "^2.1.0",
